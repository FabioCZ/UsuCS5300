function simpleFunction(a: integer; b: char) : integer;
CONST
x = 12;
BEGIN
	if(x < 15) then
	write("in if");
	return 6;
	end;
	write(x,'\n');
  write("Entered Function. Returning 5.\n");
  return 5;
END;

procedure test();
BEGIN
write("in test\n");
return;
END;

BEGIN
	test();
  write(simpleFunction(1,'e'),'\n');
END.
