CONST
x = 12;
y = "test ";
fg = false;
c = 'h';

VAR 
a,b:integer;
z : boolean;

BEGIN
write('x',x);
z := true;
a := 3;
a := succ(x);
b := pred(b);

z := ~z;
write('f',z);
z := ~z;
write('t',z,'\n','\n');
write('a',a,'b',b,'z',z,'\n');


write("test should be here ",y,fg, '\n','\n');
a := a + b; $a=3
b := a + b; $b=5
write(a,b);

a := a * b; $a=15
b := a * b; $a=75
a := 5;
b := 6;
write("less than test", a > b);

a := a + b;
write("old c",c);
read(c);
write("new c",c);
stop
END.
