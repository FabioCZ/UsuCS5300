CONST
x = 12;
y = "test";
VAR 
a,b:integer;
z : boolean;

BEGIN
z := true;
a := 3;
a := succ(y);
b := pred(b);
write('t',z);
z := ~z;
write('f',z);
z := ~z;
write('t',z,'\n','\n');
write('a',a,'b',b,'z',z,'\n');

a := a + b; $a=3
b := a + b; $b=5
write(a,b);

a := a * b; $a=15
b := a * b; $a=75
write(a,b);

a := a + b;
END.
