const
	NUMS_LENGTH = 12;
	Test = "assadfg";
type
 rt = record
        a : integer;
        b : char;
 end;
 b = record
	g : rt;
end;
 ctype = array[0:NUMS_LENGTH] of integer;
var 
	x : integer;
	y : rt;
	z : b;
	t : ctype;
begin
	$write(Test,NUMS_LENGTH);
	t[2] := 6;
	$y.a := 5;
	$y.b := 'd';
	$x := 6;
	$z.g.a := 9;
	$write(y.a, " char: ",y.b," six: ",x, " nine: ",z.g.a,'\n');
	read(x);
	write("array access: ",t[x]);
end.
